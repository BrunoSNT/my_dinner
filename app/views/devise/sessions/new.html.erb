<div class = "cont font">
	<div class = "conimg"></div>
	<div class = "conput">
		<div class = "dinner">
			<h1 class ="title font-logo2"><b>dinnerdash<b></h1>
		</div>
			<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

				<%= f.label :email, class: "labels" %><br />
				<%= f.email_field :email, autofocus: true, autocomplete: "email", class: "input" %>

				<%= f.label :senha, class: "labels" %><br />
				<%= f.password_field :password, autocomplete: "current-password", class: "input" %>

			<% if devise_mapping.rememberable? -%>

				<%= f.check_box :remember_me %>
				<%= f.label :lembrar %>

			<% end -%>

				<%= f.submit "Fazer login", class: "login" %>
			<% end %>
			<hr>
			<h5 class="">NÃ£o possui conta?</h5>
			<%= render "devise/shared/links" %>
	</div>
</div>